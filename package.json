{
  "name": "@jcoinecosystems/jcoin-token",
  "version": "1.0.0",
  "description": "Jcoin (JCN) Token",
  "keywords": [
    "jcoinecosystems",
    "jcoin-token",
    "jcoin",
    "jcn"
  ],
  "private": true,
  "license": "MIT",
  "author": {
    "name": "Jcoins.io",
    "email": "hi@jcoins.io",
    "url": "https://jcoins.io"
  },
  "homepage": "https://github.com/JcoinEcosystems/jcoin-token#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/JcoinEcosystems/jcoin-token.git"
  },
  "bugs": {
    "url": "https://github.com/JcoinEcosystems/jcoin-token/issues",
    "email": "hi@jcoins.io"
  },
  "scripts": {
    "clean": "rimraf ./build/ && mkdirp ./build/ && rimraf ./flatten/ && mkdirp ./flatten/",
    "precompile": "npm run clean",
    "compile": "truffle compile",
    "postcompile": "npm run flatten",
    "deploy": "truffle migrate --network",
    "verify": "truffle run verify --verifiers=etherscan --network",
    "test": "truffle test",
    "flatten": "waffle flatten",
    "contract-size": "truffle run contract-size",
    "ganache": "node ./scripts/ganache.js"
  },
  "files": [
    "contracts",
    "build"
  ],
  "engines": {
    "node": "18"
  },
  "dependencies": {
    "@openzeppelin/contracts": "5.0.2",
    "@openzeppelin/contracts-upgradeable": "5.0.2"
  },
  "devDependencies": {
    "@openzeppelin/truffle-upgrades": "^1.21.0",
    "@truffle/hdwallet-provider": "^2.1.15",
    "dotenv": "^16.4.5",
    "ethereum-waffle": "^4.0.10",
    "ethers": "^6.12.1",
    "ganache": "^7.9.2",
    "mkdirp": "^3.0.1",
    "rimraf": "^5.0.7",
    "truffle": "^5.11.5",
    "truffle-contract-size": "^2.0.1",
    "truffle-plugin-verify": "0.6.7"
  }
}